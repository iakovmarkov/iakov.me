{
  "name": "iakov.me",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format": "prettier ./*.{js,ts,tsx} components/**/*.{ts,tsx} pages/**/*.{ts,tsx} static/**/*.{ts,tsx} utils/**/*.{ts,tsx} --write",
    "lint": "tslint components/**/*.{ts,tsx} pages/**/*.{ts,tsx} static/**/*.{ts,tsx} utils/**/*.{ts,tsx}",
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,css}": [
      "yarn format",
      "yarn lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@types/node": "^13.5.0",
    "@types/react": "^16.9.19",
    "@zeit/next-css": "^1.0.1",
    "esm": "^3.2.25",
    "gray-matter": "^4.0.2",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.2",
    "lodash": "^4.17.15",
    "next": "9.3.2",
    "normalize.css": "^8.0.1",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-jss": "^10.0.3",
    "react-markdown": "^4.3.1",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
}
